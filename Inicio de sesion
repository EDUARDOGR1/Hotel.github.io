<script>
function Inicio()
{
var a=document.getElementById('username').value
var b=document.getElementById('contraseña').value
if(a==""||b=="")
{
alert("Por favor, rellene los detalles");
}
else 
if(a=="EduardoGR" && b=="EDUBATEMANIQ180")
{
window.open("gerente.html");
}
else
if(a=="AlanEstrada" && b=="AlanPDJ0Zzz")
{
window.open("guest.html");
}
else
if(a=="RODRIGO" && b=="XxRODRIGOJ0T0xX")
{
window.open("gerente.html");
}
else
if(a=="Julio" && b=="XxJulioG0DxX")
{
window.open("gerente.html");
}
else
if(a=="Aaron" && b=="RUIE34withAaron")
{
window.open("gerente.html");
}
else
{
alert("ID / contraseña incorrecta");
}
}
</script>
